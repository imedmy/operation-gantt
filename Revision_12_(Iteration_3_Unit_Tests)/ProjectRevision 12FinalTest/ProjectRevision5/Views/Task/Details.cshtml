@model ProjectRevision5.Models.Task

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<fieldset>
    <legend>Task</legend>

    <div class="display-label">Row Id</div>
    <div class="display-field">
            @Html.DisplayFor(model => model.Row_ID)
    </div>

    <div class="display-label">Task_Name</div>
    <div class="display-field">
            @Html.DisplayFor(model => model.Task_Name)
    </div>

    <div class="display-label">Start_Time</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Start_Time)
    </div>

    <div class="display-label">End_Time</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.End_Time)
    </div>

    <div class="display-label">Predecessor</div>
    <div class="display-field">
        @Html.DisplayFor(model => model.Predecessor)
    </div>

</fieldset>
<p>
    @Html.ActionLink("Edit", "Edit", new { id=Model.Row_ID }) |
    @Html.ActionLink("Back to List", "Index")
</p>
